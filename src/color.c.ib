#include <stdio.h>
#include "pair.h"
#include "color.h"

void set_color(const tcolor c)
	const int code = 16 + (36 * (int) (c.r /255.0 * 5)) + ( 6 * (int) (c.g / 255.0 * 5)) + (int) (c.b / 255.0 * 5)
	printf("\033[38;5;%dm", code)

void set_bold(void)
	printf("\033[1m")

void set_blank(void)
	printf("\033[0m")
